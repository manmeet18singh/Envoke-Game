# Auto detect text files and perform LF normalization
* text=auto
*.csproj filter=lfs diff=lfs merge=lfs -text
*.csproj.user filter=lfs diff=lfs merge=lfs -text
*.sln filter=lfs diff=lfs merge=lfs -text
*.bs filter=lfs diff=lfs merge=lfs -text
*.bc filter=lfs diff=lfs merge=lfs -text
*.assets.resS filter=lfs diff=lfs merge=lfs -text
ArtifactDB filter=lfs diff=lfs merge=lfs -text
